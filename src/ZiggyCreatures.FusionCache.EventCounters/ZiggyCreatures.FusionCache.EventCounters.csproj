<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <Version>0.1.2.32-appmetrics</Version>
    <Authors>Joe Shook</Authors>
    <Product>FusionCache</Product>
    <PackageId>ZiggyCreatures.FusionCache.EventCounters</PackageId>
    <IncludeSource>True</IncludeSource>
    <Copyright>Copyright Â© Jody Donetti and Joe Shook 2021</Copyright>
    <PackageIcon>logo-128x128.png</PackageIcon>
    <Description>App.Metrics provider implementation of IFusionMetrics</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>caching;cache;multi-level;multilevel;fusion;fusioncache;fusion-cache;performance;async;ziggy;metrics;EventSource;EventCounter;DiagnosticCounter</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RootNamespace>ZiggyCreatures.FusionCache.EventCounters</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>ZiggyCreatures.FusionCache.xml</DocumentationFile>
    <UserSecretsId>5d706789-eb3f-4665-8f7b-7a951f21e9d4</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <None Include="artwork\logo-128x128.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="3.1.13" />
    <PackageReference Include="ZiggyCreatures.FusionCache" Version="0.1.2.36-appmetrics" />
  </ItemGroup>

</Project>
